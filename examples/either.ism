module Example {
-- Example tprob file.

data Either (p :: ⋆) (q :: ⋆) = Left p | Right q

sig withEither :: forall (p :: ⋆) (q :: ⋆) (r ∷ ⋆)
                  . (p -> r) -> (q -> r) -> Either p q -> r
fun withEither l r t = case t of
  { (Left x) -> l x
  ; (Right y) -> r y
  }

}
